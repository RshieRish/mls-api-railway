# MLS PIN Automation Configuration
# Copy this file to .env and fill in your actual credentials

# Database Configuration (should already exist)
DATABASE_URL=postgresql://username:password@localhost:5432/database_name

# MLS PIN Website Credentials
# You need to obtain these from your MLS PIN account
MLS_PIN_URL=https://www.mlspin.com
MLS_PIN_USERNAME=your_username_here
MLS_PIN_PASSWORD=your_password_here

# Automation Settings
UPSERT_BATCH_SIZE=100
DOWNLOAD_TIMEOUT=300
MAX_RETRIES=3
RETRY_DELAY=60

# Scheduler Settings
DEFAULT_INTERVAL_HOURS=1
MAX_CONCURRENT_JOBS=1
JOB_TIMEOUT=1800  # 30 minutes

# File Management
MAX_FILE_AGE_DAYS=7
CLEANUP_HOUR=2  # 2 AM daily cleanup

# Logging
LOG_LEVEL=INFO
LOG_FILE=automation.log
MAX_LOG_SIZE=10485760  # 10MB
LOG_BACKUP_COUNT=5

# Brandon's Agent Configuration
BRANDON_AGENT_ID=Cn222505

# Security
# Set to true in production
HEADLESS_BROWSER=true
DISABLE_BROWSER_SANDBOX=false